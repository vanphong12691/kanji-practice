{
  "name": "kanji-app",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "build1": "electron-packager . dist --platform=win32 --arch=x64 --overwrite to force",
    "build": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/images/icon.png",
    "package:installer": "node ./script/installer.js"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "PHONG",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.6.6",
    "devtron": "^1.3.0",
    "electron-winstaller": "^2.2.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "ejs": "^2.7.1",
    "electron-json-storage": "^4.1.8",
    "electron-packager": "^14.0.6",
    "jquery": "^3.5.0"
  }
}
